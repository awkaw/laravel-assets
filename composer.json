{
    "name": "awkaw/laravel-assets",
    "description": "Compile Assets for Laravel",
    "license" : "BSD-3-Clause",
    "keywords": ["assets", "laravel"],
    "authors" : [
        {
            "name": "Alexander Krupnov",
            "email": "awkaw@yandex.ru",
            "homepage": "https://github.com/awkaw"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8.35 || ^5.0 || ^6.0 || ^7.0"
    },
    "autoload": {
        "psr-4": {
            "LaravelAssets\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.0-dev"
        },
        "laravel": {
            "providers": [
                "LaravelAssets\\ServiceProvider"
            ]
        }
    }
}
